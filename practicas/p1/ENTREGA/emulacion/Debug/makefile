-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
emulacion.hex \

SECONDARY_LIST += \
emulacion.lst \

SECONDARY_SIZE += \
emulacion.siz \


# All Target
all: emulacion.elf secondary-outputs

# Tool invocations
emulacion.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: ARM Sourcery Windows GCC C Linker'
	arm-none-eabi-gcc -T"C:\hlocal3\workspace\emulacion\common\ld_script.ld" -nostartfiles -Wl,-Map,emulacion.map -mcpu=arm7tdmi -g3 -gdwarf-2 -o "emulacion.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

emulacion.hex: emulacion.elf
	@echo 'Invoking: ARM Sourcery Windows GNU Create Flash Image'
	arm-none-eabi-objcopy -O binary emulacion.elf  "emulacion.hex"
	@echo 'Finished building: $@'
	@echo ' '

emulacion.lst: emulacion.elf
	@echo 'Invoking: ARM Sourcery Windows GNU Create Listing'
	arm-none-eabi-objdump -h -S emulacion.elf > "emulacion.lst"
	@echo 'Finished building: $@'
	@echo ' '

emulacion.siz: emulacion.elf
	@echo 'Invoking: ARM Sourcery Windows GNU Print Size'
	arm-none-eabi-size  --format=berkeley emulacion.elf
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(SECONDARY_SIZE)$(OBJS)$(C_DEPS)$(ASM_DEPS)$(SECONDARY_FLASH)$(EXECUTABLES)$(SECONDARY_LIST)$(S_UPPER_DEPS) emulacion.elf
	-@echo ' '

secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

.PHONY: all clean dependents
.SECONDARY:

-include ../makefile.targets
